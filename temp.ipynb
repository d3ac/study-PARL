   "source": [
    "import torch\n",
    "\n",
    "# 创建一个张量并设置requires_grad=True使其可微\n",
    "x = torch.tensor([1.0, 2.0, 3.0], requires_grad=True)\n",
    "\n",
    "# 定义一个简单的操作\n",
    "y = x * 2\n",
    "\n",
    "# 计算操作的结果的和\n",
    "z = y.sum()\n",
    "\n",
    "# 反向传播，计算梯度\n",
    "z.backward()\n",
    "\n",
    "# 打印x的梯度\n",
    "print(x.grad)"
   ]
